<h1>New Tide Extreme</h1>

<%= render 'form', tide_extreme: @tide_extreme %>

<%= form_for(@tide_extreme) do |f| %>
  <%= f.label :time %>
  <%= f.text_field :time %>
  <%= f.label :type %>
  <%= f.text_field :type %>
  <%= f.label :height %>
  <%= f.number_field :height %>
  <%= f.submit %>
<% end %>

<%= link_to 'Back', tide_extremes_path %>
